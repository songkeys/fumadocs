{
  "name": "fumadocs-mdx",
  "version": "10.0.2",
  "description": "The built-in source for Fumadocs",
  "keywords": [
    "NextJs",
    "Docs"
  ],
  "homepage": "https://fumadocs.vercel.app",
  "repository": "github:fuma-nama/fumadocs",
  "license": "MIT",
  "author": "Fuma Nama",
  "exports": {
    "./loader-mdx": "./loader-mdx.js",
    "./config": {
      "import": "./dist/config/index.mjs",
      "types": "./dist/config/index.d.mts"
    },
    "./next": {
      "import": "./dist/next/index.mjs",
      "types": "./dist/next/index.d.mts"
    },
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.mts"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.mts",
  "typesVersions": {
    "*": {
      "config": [
        "./dist/config/index.d.mts"
      ],
      "next": [
        "./dist/next/index.d.mts"
      ]
    }
  },
  "bin": "./bin.mjs",
  "files": [
    "dist/*",
    "loader-mdx.js",
    "bin.mjs"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "chokidar": "^4.0.1",
    "cross-spawn": "^7.0.3",
    "esbuild": "^0.24.0",
    "estree-util-value-to-estree": "^3.1.2",
    "fast-glob": "^3.3.1",
    "gray-matter": "^4.0.3",
    "micromatch": "^4.0.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/mdast": "^4.0.3",
    "@types/mdx": "^2.0.13",
    "@types/micromatch": "^4.0.9",
    "@types/react": "^18.3.10",
    "eslint-config-custom": "workspace:*",
    "fumadocs-core": "workspace:*",
    "next": "15.0.0-rc.0",
    "tsconfig": "workspace:*",
    "unified": "^11.0.5",
    "webpack": "^5.90.3"
  },
  "peerDependencies": {
    "fumadocs-core": "^13.2.1",
    "next": "14.x.x || 15.x.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
